comment: 'PC 1440kB 3.5" 80-track 18-sector DSHD'

image_reader {
	filename: "ibm1440.img"
	type: IMG
}

image_writer {
	filename: "ibm1440.img"
	type: IMG
}

layout {
	tracks: 80
	sides: 2
	layoutdata {
		sector_size: 512
		physical {
			start_sector: 1
			count: 18
		}
	}
}

encoder {
	ibm {
		trackdata {
			target_rotational_period_ms: 200
			target_clock_period_us: 2
		}
	}
}

decoder {
	ibm {}
}

filesystem {
	fatfs {}
}

