comment: 'Brother 240kB 3.5" 78-track SS GCR'

image_reader {
	filename: "brother240.img"
	type: IMG
}

image_writer {
	filename: "brother240.img"
	type: IMG
}

layout {
	tracks: 78
	sides: 1
	layoutdata {
		sector_size: 256
		physical {
			start_sector: 0
			count: 12
			#skew: 5
		}
	}
}

encoder {
	brother {}
}

decoder {
	brother {}
}

drive {
	head_bias: 3
}

filesystem {
	type: FATFS
}

