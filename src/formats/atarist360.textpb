comment: 'Atari ST 360kB 3.5" 80-track 9-sector SSDD'

image_reader {
	filename: "atarist360.st"
	img {}
}

image_writer {
	filename: "atarist360.st"
	img {}
}

layout {
	tracks: 80
	sides: 1
	trackdata {
		sector_size: 512
		sector_range {
			start_sector: 1
			sector_count: 9
		}
	}
}

encoder {
	ibm {
		trackdata {
			target_rotational_period_ms: 200
			target_clock_period_us: 4
			emit_iam: false
			gap0: 80
			gap2: 22
			gap3: 34
			sector_size: 512
			sectors {
				sector: 1
				sector: 2
				sector: 3
				sector: 4
				sector: 5
				sector: 6
				sector: 7
				sector: 8
				sector: 9
			}
		}
	}
}

decoder {
	ibm {
		trackdata {
			ignore_sector: 66
			sector_range {
				min_sector: 1
				max_sector: 9
			}
		}
	}
}

tracks {
	start: 0
	end: 79
}

heads {
	start: 0
	end: 0
}

filesystem {
	fatfs {}
}

