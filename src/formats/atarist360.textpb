comment: 'Atari ST 360kB 3.5" 80-track 9-sector SSDD'

include: '_atari'

image_reader {
	filename: "atarist360.st"
	type: IMG
}

image_writer {
	filename: "atarist360.st"
	type: IMG
}

layout {
	tracks: 80
	sides: 1
	layoutdata {
		sector_size: 512
		physical {
			start_sector: 1
			count: 9
		}
	}
}
