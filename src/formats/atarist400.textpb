comment: 'Atari ST 400kB 3.5" 80-track 10-sector SSDD'

include: '_atari'

image_reader {
	filename: "atarist400.st"
	type: IMG
}

image_writer {
	filename: "atarist400.st"
	type: IMG
}

layout {
	tracks: 80
	sides: 1
	layoutdata {
		sector_size: 512
		physical {
			start_sector: 1
			count: 10
		}
	}
}
