comment: 'Brother 120kB 3.5" 39-track SS GCR'

image_reader {
	filename: "brother120.img"
	type: IMG
}

image_writer {
	filename: "brother120.img"
	type: IMG
}

layout {
	tracks: 39
	sides: 1
	layoutdata {
		sector_size: 256
		physical {
			start_sector: 0
			count: 12
			skew: 5
		}
	}
}

encoder {
	brother {
		format: BROTHER120
	}
}

decoder {
	brother {}
}

drive {
	head_bias: 0
	group_offset: 1
}

tpi: 48

filesystem {
	brother120 {}
}

